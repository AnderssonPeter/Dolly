namespace Dolly;
partial class ComplexClass : IClonable<ComplexClass>
{
    object ICloneable.Clone() => this.DeepClone();
    public virtual ComplexClass DeepClone() =>
        new ()
        {
            ReferenceTypeNotNull = ReferenceTypeNotNull.DeepClone(),
            ReferenceTypeNull = ReferenceTypeNull?.DeepClone(),
            StringReferenceTypeNotNull = StringReferenceTypeNotNull,
            StringReferenceTypeNull = StringReferenceTypeNull,
            ValueTypeNotNull = ValueTypeNotNull.DeepClone(),
            ValueTypeNull = ValueTypeNull?.DeepClone(),
            IntValueTypeNotNull = IntValueTypeNotNull,
            IntValueTypeNull = IntValueTypeNull
