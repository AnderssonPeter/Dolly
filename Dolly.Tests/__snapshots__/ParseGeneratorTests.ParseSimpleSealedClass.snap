using Dolly;
using System.Linq;
namespace Dolly;
partial class SimpleClass : IClonable<SimpleClass>
{
    object ICloneable.Clone() => this.DeepClone();
    public SimpleClass DeepClone() =>
        new ()
        {
            First = First,
            Second = Second
        };

    public SimpleClass ShallowClone() =>
        new ()
        {
            First = First,
            Second = Second
        };
}
